cacheSolve <- function(x, ...) 
  {
 
  ##give a matrix inverse of x matrix
  
  inver <- x$getinver()
  if(!is.null(inver)) 
  {
    
    message("Getting Cached Data")
    return(inver)
  }
  data <- x$get()
  inver <- solve(data)
  x$setinver(inver)
  inver
}
